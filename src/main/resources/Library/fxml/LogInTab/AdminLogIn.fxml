<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane stylesheets="@../../css/loginwindow.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Library.ui.LogIn.AdminLogInController">
   <children>
      <VBox alignment="CENTER" prefHeight="650.0" prefWidth="500.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="500.0">
               <children>
                  <Button id="back" fx:id="backButton" graphicTextGap="5.0" mnemonicParsing="false" onAction="#goBack" prefHeight="26.0" prefWidth="82.0" text="Back">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                     <graphic>
                        <ImageView fitHeight="15.0" fitWidth="13.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../icon/back-icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
            <VBox alignment="CENTER" prefHeight="630.0" prefWidth="500.0">
               <children>
                  <Button id="submitButton" fx:id="submitButton" mnemonicParsing="false" onAction="#submit" prefHeight="46.0" prefWidth="206.0" text="ĐĂNG NHẬP" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
